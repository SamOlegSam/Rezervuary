<div class="modal-header" style="text-align: center; background-color: #D0EDFE; border-radius:5px;">
    <center><h5> Сохранение данных</h5></center>
</div>
<div class="modal-body">
    @{ if (ViewBag.Message == "ЗАПИСЬ НА ДАННУЮ ДАТУ УЖЕ СУЩЕСТВУЕТ")
        {
            
    <h3 style="color:firebrick; text-align:center"><b>ЗАПИСЬ НА ДАННУЮ ДАТУ УЖЕ СУЩЕСТВУЕТ</b></h3>
                
        }
        else
        {
            <center>@ViewBag.Message</center>
        }
      }
</div>
<div class="modal-footer" style="text-align:center">

    <button type="button" onclick="window.location.reload()" class="btn btn-default" data-dismiss="modal" style="min-width:100px">OK</button>
</div>
